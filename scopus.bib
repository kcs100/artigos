@ARTICLE{Teichgräber2021701,
	author = {Teichgräber, Ulf and Ingwersen, Maja and Mentzel, Hans-Joachim and Aschenbach, Rene and Neumann, Rotraud and Franiel, Tobias and Herzog, Aimée Barbara and Böttcher, Joachim and Pfeil, Alexander and Mensel, Birger and Kühnel, Christian and Freesmeyer, Martin and Fischer, Martin R. and Zottmann, Jan},
	title = {Impact of a Heutagogical, Multimedia-Based Teaching Concept to Promote Self-Determined, Cooperative Student Learning in Clinical Radiology},
