@article{ WOS:000975454900023,
Author = {Cherusheva, Galyna and Nowak, Barbara and Maksymenko, Anatolii and
   Kabysh, Maryna and Vakerych, Mykhailo},
Title = {Higher pedagogical education in the European Union: Innovative
   technologies Educaci?n pedag?gica superior en la Uni?n Europea:
   Tecnolog?as innovadoras},
