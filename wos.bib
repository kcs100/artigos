Author = {Knoechelmann, Anne Caroline and Becker, Jan-Niklas and Stiller, Gerald
   and Steinmann, Diana and Behrends, Marianne},
Title = {Digital redesign of the radiotherapy course for medical students with a
   blended learning approach},
Journal = {STRAHLENTHERAPIE UND ONKOLOGIE},
