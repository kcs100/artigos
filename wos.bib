@article{ WOS:000298611100003,
Author = {Mazzotti, Valerie L. and Wood, Charles L. and Test, David W. and Fowler,
   Catherine H.},
Title = {Effects of Computer-Assisted Instruction on Students' Knowledge of the
   Self-Determined Learning Model of Instruction and Disruptive Behavior},
Journal = {JOURNAL OF SPECIAL EDUCATION},
Year = {2012},
Volume = {45},
Number = {4},
Pages = {216-226},
Month = {FEB},
Abstract = {Instruction about goal setting can increase students' self-determination
   and reduce problem behavior. Computer-assisted instruction could offer
   teachers another format for teaching goal setting and
   self-determination. This study used a multiple probes across
   participants design to examine the effects of computer-assisted
   instruction on students' knowledge of the Self-Determined Learning Model
   of Instruction (SDLMI) and level of disruptive behavior. Results
   indicated increased knowledge of the SDLMI and decreased levels of
   disruptive behavior for all participants. Implications for future
   research and practice are discussed.},
