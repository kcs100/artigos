   of self-determined},
Booktitle = {PROCEEDINGS OF THE IEEE INTERNATIONAL CONFERENCE ON ADVANCED MATERIALS
   FOR SCIENCE AND ENGINEERING (IEEE-ICAMSE 2016)},
Year = {2016},
Pages = {188-191},
Note = {IEEE International Conference on Advanced Materials for Science and
