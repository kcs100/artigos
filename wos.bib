@inproceedings{ WOS:000402955906091,
Author = {Koseler, Refika and Ben-Daat, Hagit and Austin, Ara C. and Gould, Ian R.},
Editor = {Chova, LG and Martinez, AL and Torres, IC},
Title = {A WEB-BASED TEACHING TOOL FOR MULTI-STEP SYNTHESIS IN ORGANIC CHEMISTRY:
   STUDENT PERSPECTIVES AND MOTIVATIONS},
Booktitle = {EDULEARN16: 8TH INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
