@article{ WOS:000308228600005,
Author = {Shogren, Karrie A. and Palmer, Susan B. and Wehmeyer, Michael L. and
   Williams-Diehm, Kendra and Little, Todd D.},
Title = {Effect of Intervention With the Self-Determined Learning Model of
   Instruction on Access and Goal Attainment},
Journal = {REMEDIAL AND SPECIAL EDUCATION},
Year = {2012},
Volume = {33},
Number = {5},
Pages = {320-330},
Month = {SEP-OCT},
Abstract = {Promoting self-determination has been identified as best practice in
   special education and transition services and as a means to promote goal
   attainment and access to the general education curriculum for students
   with disabilities. There have been, however, limited evaluations of the
   effects of interventions to promote self-determination on outcomes
   related to access to the general education curriculum. This article
