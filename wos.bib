@inproceedings{ WOS:000369659200024,
Author = {Mac Callum, Kathryn and Jeffery, Lynn and Kinshuk},
Editor = {Brown, TH and VanDerMerwe, HJ},
Title = {Heutagogial Approaches in the Understanding and Modelling the Adoption
   of Mobile Learning},
Booktitle = {MOBILE LEARNING VOYAGE - FROM SMALL RIPPLES TO MASSIVE OPEN WATERS},
