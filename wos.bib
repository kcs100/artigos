Title = {Blending MOOCs into Higher Education Courses - a Case Study},
Booktitle = {PROCEEDINGS OF 2018 LEARNING WITH MOOCS (IEEE LWMOOCS 2018): MOOCS FOR
   ALL - A SOCIAL AND INTERNATIONAL APPROACH},
Year = {2018},
Pages = {134-136},
