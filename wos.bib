@article{ WOS:000882153200009,
Author = {Sidhu, Gurnam Kaur and Nawi, Sofwah Md and Ramakrishnan, Kamalanathan
   and Ruofei, Du},
Title = {Developing PG Students' Learner Autonomy through the PAH-Continuum: A
   case study},
Journal = {ENVIRONMENT-BEHAVIOUR PROCEEDINGS JOURNAL},
Year = {2022},
Volume = {7},
Number = {21},
Month = {SEP},
Note = {10th Asia-Pacific International Conference on E-B Studies (AicE-Bs),
   Kota Kinabalu, MALAYSIA, SEP 07-08, 2022},
Abstract = {To aid and better understand the current postgraduate educational
   learning environment, this study examined the implementation of the
   Pedagogy-Andragogy-Heutagogy (PAH) Continuum as an instructional
   framework for developing competencies and learner autonomy among
   postgraduate (PG) students. This is in response to the high attrition
   rates and postgraduate students' failure to graduate on time, afflicting
   the quality of PG education in Malaysia. A mixed-method descriptive case
   study design was employed, involving pre and post-tests, a
   questionnaire, and focus-group interviews. The findings revealed
   significant improvement in the students' critical reading and research
   skills, while their autonomy level was moderate due to personal and
   cultural limitations.},
